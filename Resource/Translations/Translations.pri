# For autocompiling qm-files.

CONFIG *= lrelease embed_translations
QM_FILES_RESOURCE_PREFIX = Translations
#QM_FILES_INSTALL_PATH = Translations
TRANSLATIONS = \
    $$PWD/app_zh_CN.ts \
    $$PWD/app_zh_TW.ts \
    $$PWD/app_cs.ts \
    $$PWD/app_da.ts \
    $$PWD/app_de.ts \
    $$PWD/app_fr.ts \
    $$PWD/app_hu.ts \
    $$PWD/app_ja.ts \
    $$PWD/app_ko.ts \
    $$PWD/app_pl.ts \
    $$PWD/app_ru.ts \
    $$PWD/app_sk.ts \
    $$PWD/app_sl.ts \
    $$PWD/app_uk.ts
